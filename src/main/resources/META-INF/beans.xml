<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define your beans here -->
    <bean id="userRepository" class="com.youcode.marjan.Repositories.UserRepository">
        <!-- Set necessary properties or constructor arguments -->
    </bean>

    <bean id="userService" class="com.youcode.marjan.Services.UserService">
        <constructor-arg ref="userRepository"/>
    </bean>

    <!-- Add more beans as needed -->

</beans>
